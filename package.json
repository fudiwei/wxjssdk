        "eslint-plugin-standard": "^5.0.0",
        "gulp": "^4.0.2",
        "gulp-babel": "^8.0.0",
        "gulp-eslint": "^6.0.0",
        "gulp-rimraf": "^1.0.0",
        "gulp-strip-debug": "^4.0.0",
